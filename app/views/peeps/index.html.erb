<h1>Listing peeps</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @peeps.each do |peep| %>
  <tr>
    <td><%= peep.name %></td>
    <td><%= peep.email %></td>
    <td><%= link_to 'Show', peep %></td>
    <td><%= link_to 'Edit', edit_peep_path(peep) %></td>
    <td><%= link_to 'Destroy', peep, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Peep', new_peep_path %>
